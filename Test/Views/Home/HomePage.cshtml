@{
    ViewData["Title"] = "Загрузка";
}

<header>
    <h1>Загрузка архивов погодных условий в Москве</h1>
</header>
<div>
    <form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="LoadData">
        <label>Выберите файл:</label>
        <input type="file" name="weatherFiles" id="weatherFiles" multiple><br><br>
        <button type="submit">Загрузить</button>
        <h3>@ViewBag.Message</h3>
    </form>
</div>
